{
  "manifest_version": 2,
  "name": "Github Dark Theme",
  "short_name": "Github Dark Theme",
  "description": "How to protect your eyes from computer screens?",
  "version": "0.9.25",
  "applications": {
    "gecko": {
      "id": "{1dbe3fd3-f191-4a3b-a5d7-a6f95ed2aea1}"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://gist.github.com/*"
      ],
      "run_at": "document_start",
      "css": [
        "app/app.css"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}