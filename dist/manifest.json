{
  "manifest_version": 2,
  "name": "Github Dark Theme",
  "short_name": "Github Dark Theme",
  "description": "A Dark theme for all of Github based on Atom One Dark.",
  "version": "0.9.52",
  "browser_action": {
    "default_title": "Github Dark Theme",
    "default_popup": "app/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.github.com/*"
      ],
      "run_at": "document_end",
      "css": [
        "app/app.css"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com 'unsafe-eval'; object-src 'self'"
}
